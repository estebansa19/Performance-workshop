!!!
%html
  %head
    %meta{:content => "text/html; charset=UTF-8", "http-equiv" => "Content-Type"}/
    %title Performance Workshop Ayenda
    = csrf_meta_tags
    = csp_meta_tag
    = stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload'
    = javascript_include_tag 'application', 'data-turbolinks-track': 'reload'

    %script{:crossorigin => "anonymous", :integrity => "sha256-WpOohJOqMqqyKL9FccASB9O0KwACQJpFTUBLTYOVvVU=", :src => "https://code.jquery.com/jquery-3.4.1.js", defer: true}
    %script{src: "https://cdnjs.cloudflare.com/ajax/libs/react/16.8.6/cjs/react.production.min.js", async: true}
    %script{src: "https://cdnjs.cloudflare.com/ajax/libs/react-router-dom/5.0.1/react-router-dom.js", async: true}
    %script{src: "https://cdnjs.cloudflare.com/ajax/libs/react-router/5.0.1/react-router.min.js", async: true}
    %script{src: "https://cdnjs.cloudflare.com/ajax/libs/angular.js/1.7.8/angular.js", async: true}
    %script{src: "https://cdnjs.cloudflare.com/ajax/libs/vue/2.6.10/vue.js", async: true}
  %body
    = render 'layouts/partials/navbar'
    - if notice
      .container.mt-2
        .alert.alert-success.alert-dismissible.fade.show{:role => "alert"}
          %strong= notice
          %button.close{"aria-label" => "Close", "data-dismiss" => "alert", :type => "button"}
            %span{"aria-hidden" => "true"} Ã—
    = yield
    = render 'layouts/partials/footer'

-# Async CSS script and lazy load script
-# :javascript
